<?xml version="1.0" encoding="utf-8"?>
<interface>
	<object class="GtkMenu" id="entryview_popup_menu">
		<property name="visible">True</property>
		<property name="can_focus">False</property>
		<child>
			<object class="GtkMenuItem" id="pleer_copy_uri">
				<property name="label">Copy URI</property>
				<property name="tooltip_text">Copy URI to clipboard</property>
				<property name="visible">True</property>
				<property name="use_action_appearance">False</property>
				<signal name="activate" handler="copy2clipboard" swapped="no" />
			</object>
		</child>
		<child>
			<object class="GtkMenuItem" id="pleer_download_song">
				<property name="label">Download</property>
				<property name="tooltip_text">Download this song</property>
				<property name="visible">True</property>
				<property name="use_underline">True</property>
				<property name="use_action_appearance">False</property>
				<signal name="activate" handler="download_song" swapped="no"/>
			</object>
		</child>
	</object>
</interface>